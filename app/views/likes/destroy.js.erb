$("#like-form-javascript-<%= @micropost.id %>").html('<%= escape_javascript(link_to "", likes_path(micropost_id: @micropost.id), method: :post, remote: true, class: "glyphicon glyphicon-heart-empty") %>');
$("#like-stats-javascript-<%= @micropost.id %>").html('<%= @micropost.likes.count %>');
$("#javascript-like-ranking").html('<%= escape_javascript(render "shared/like_ranking", third_counts_of_likes_count: @third_counts_of_likes_count, third_likes_counts_microposts: @third_likes_counts_microposts) %>');